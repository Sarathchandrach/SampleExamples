<!-- Reset DIALOG -->
<Dialog xmlns="sap.m" xmlns:layout="sap.ui.layout" class="dialogBox"
	id="RefreshDialog" title="{i18n>RESET_TITLE}">
	<content>
		<layout:VerticalLayout class="settingPad">
			<layout:content>
				<Text id="resetText" text="{i18n>LBL_CONFIRM_REFRESH}"></Text>
			</layout:content>
		</layout:VerticalLayout>
		<!-- <VBox class="settingPad"> <items> <Text id="resetText" text="{i18n>LBL_CONFIRM_REFRESH}"></Text> 
			</items> </VBox> -->
	</content>
	<beginButton>
		<Button id="rfButOK" text="{i18n>BTN_RF_OK}" press="refreshDlg"></Button>
	</beginButton>
	<endButton>
		<Button id="rfButCancel" text="{i18n>BTN_RF_CANCEL}" press="refreshDlg"></Button>
	</endButton>
</Dialog>