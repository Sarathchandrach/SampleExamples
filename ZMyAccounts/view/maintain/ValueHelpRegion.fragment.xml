<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:layout="sap.ui.layout">
  
  <SelectDialog
  	if="selectDialog"
  	title="{i18n>REGION}"
    class="sapUiPopupWithPadding"
    items="{/RegionCustomizing}"
    liveChange="onRegionValueHelpSearch"
    confirm="onRegionValueHelpClose"
    cancel="onRegionValueHelpCancel">
    <StandardListItem
      title="{region}"
      />
  </SelectDialog>
   
</core:FragmentDefinition>