                                                                                                       </layoutData>
                                                                                                </Text>
                                                                                         </f:fields>
                                                                                  </f:FormElement>
                                                                                  <f:FormElement
                                                                                         id="MB_FORM_EMPLOYER_COSTS">
                                                                                         <f:layoutData>
                                                                                                <l:ResponsiveFlowLayoutData
                                                                                                       linebreak="true"
                                                                                                       data-margin="false"></l:ResponsiveFlowLayoutData>
                                                                                         </f:layoutData>
                                                                                         <f:label>
                                                                                                <Label
                                                                                                       visible="true"
                                                                                                       id="MB_LABEL_EMPLOYER_COSTS"
                                                                                                       text="{i18n>MB_EMPLOYER_COSTS}">
                                                                                                       <layoutData>
                                                                                                              <l:ResponsiveFlowLayoutData
                                                                                                                     data-weight="4"></l:ResponsiveFlowLayoutData>
                                                                                                       </layoutData>
                                                                                                </Label>
                                                                                         </f:label>
                                                                                         <f:fields>
                                                                                                <Text
                                                                                                       visible="true"
                                                                                                       id="MB_LABEL_EMPLOYER_COSTS_VALUE">
                                                                                                       <layoutData>
                                                                                                              <l:ResponsiveFlowLayoutData
                                                                                                                     data-weight="4"></l:ResponsiveFlowLayoutData>
                                                                                                       </layoutData>
                                                                                                </Text>
                                                                                         </f:fields>
                                                                                  </f:FormElement>
                                                                           </f:formElements>
                                                                     </f:FormContainer>
                                                              </f:formContainers>
                                                       </f:Form>
                                                       <List
                                                              visible="true"
                                                              id="MB_LABEL_BENEFICIARIES"
                                                              headerDesign="Plain"
                                                              showSeparators="None"
                                                              noDataText="{i18n>MB_NO_DATA}"
                                                              headerText="{i18n>MB_BENEFICIARIES}"
                                                              showNoData="true">
                                                          
                                                       </List>
                                                </VBox>
                                         </content>
                                  </IconTabFilter>
                                  <IconTabFilter
                                         icon="sap-icon://attachment"
                                         iconColor="Default"
                                         key="Attachments"
                                         id="Attachment">
                                         <content>
                                                <List
                                                       id="MB_LABEL_DOCUMENT"
                                                       showSeparators="Inner"
                                                       noDataText="{i18n>MB_NO_DATA}"
                                                       items="{attachmentsModel>/0}">
                                                       <StandardListItem
                                                              type="Active"
                                                              icon="sap-icon://chain-link"
                                                              title="{attachmentsModel>UrlPlanText}"
                                                              press="onSenderPress"></StandardListItem>
                                                </List>
                                         </content>
                                         </IconTabFilter>
                                           <!-- extension added to add icon tab filter --> 
                <core:ExtensionPoint name="extS4IconTabFilter"></core:ExtensionPoint>
                                  
                           </items>
                     </IconTabBar>
              </content>
       </Page>
</core:View>