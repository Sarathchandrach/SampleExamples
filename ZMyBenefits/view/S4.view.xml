                                                                                                       visible="true"
                                                                                                       id="MB_LABEL_POSTTAXCOST"
                                                                                                       text="{i18n>MB_EMP_POST_TAX_COST}">
                                                                                                       <layoutData>
                                                                                                              <l:ResponsiveFlowLayoutData
                                                                                                                     data-weight="4"></l:ResponsiveFlowLayoutData>
                                                                                                       </layoutData>
                                                                                                </Label>
                                                                                         </f:label>
                                                                                         <f:fields>
                                                                                                <Text
                                                                                                       visible="true"
                                                                                                       id="MB_LABEL_POSTTAXCOST_VALUE">
                                                                                                       <layoutData>
                                                                                                              <l:ResponsiveFlowLayoutData
                                                                                                                     data-weight="4"></l:ResponsiveFlowLayoutData>
                                                                                                       </layoutData>
                                                                                                </Text>
                                                                                         </f:fields>
                                                                                  </f:FormElement>
                                                                                  <f:FormElement
                                                                                         id="MB_FORM_EMP_BONUS_POST_TAX_COST">
                                                                                         <f:layoutData>
                                                                                                <l:ResponsiveFlowLayoutData
                                                                                                       linebreak="true"
                                                                                                       data-margin="false"></l:ResponsiveFlowLayoutData>
                                                                                         </f:layoutData>
                                                                                         <f:label>
                                                                                                <Label
                                                                                                       visible="true"
                                                                                                       id="MB_LABEL_EMP_BONUS_POST_TAX_COST"
