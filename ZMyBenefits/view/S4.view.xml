                                                                                         <f:fields>
                                                                                                <Text
                                                                                                       visible="true"
                                                                                                       id="MB_LABEL_PLAN_NUMBER_VALUE">
                                                                                                       <layoutData>
                                                                                                              <l:ResponsiveFlowLayoutData
                                                                                                                     data-weight="4"></l:ResponsiveFlowLayoutData>
                                                                                                       </layoutData>
                                                                                                </Text>
