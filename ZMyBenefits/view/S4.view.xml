                                                                                                              <l:ResponsiveFlowLayoutData
                                                                                                                     data-weight="4"></l:ResponsiveFlowLayoutData>
                                                                                                       </layoutData>
                                                                                                </Text>
                                                                                         </f:fields>
                                                                                  </f:FormElement>
                                                                                  <f:FormElement
                                                                                         id="MB_FORM_PRETAXCOST">
                                                                                         <f:layoutData>
                                                                                                <l:ResponsiveFlowLayoutData
                                                                                                       linebreak="true"
                                                                                                       data-margin="false"></l:ResponsiveFlowLayoutData>
                                                                                         </f:layoutData>
                                                                                         <f:label>
                                                                                                <Label
                                                                                                       visible="true"
                                                                                                       id="MB_LABEL_PRETAXCOST"
                                                                                                       text="{i18n>MB_EMP_PRE_TAX_COST}">
                                                                                                       <layoutData>
