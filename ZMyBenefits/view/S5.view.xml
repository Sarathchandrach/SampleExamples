													id="MB_FORM_BONUS_PRE_TAX_DUMMY">
													<f:layoutData>
														<l:ResponsiveFlowLayoutData
															linebreak="true"
															margin="false"></l:ResponsiveFlowLayoutData>
													</f:layoutData>
													<f:label>
														<Label
															visible="true"
															id="MB_BONUS_PRE_TAX_DUMMY">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Label>
													</f:label>
													<f:fields>
														<Text
															visible="true"
															id="MB_BONUS_PRE_TAX_TEXT"
															text="{i18n>MB_MESSAGE_PRE_TAX_TO_POST_TAX}">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Text>
													</f:fields>
												</f:FormElement>
												<f:FormElement
													id="MB_FORM_BONUS_POST_TAX">
													<f:layoutData>
