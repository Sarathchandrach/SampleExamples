	<Page
		title="{i18n>MB_APP_DETAIL_TITLE}"
		id="MB_PAGE_SAVINGS">
		<content>
			<ObjectHeader
				id="MB_SAVINGS_HEADER">
				<attributes>
					<ObjectAttribute
						id="MB_PLAN_SUB_TYPE_TEXT"/>
					<ObjectAttribute
						id="MB_PLAN_OPTION_TEXT" />
					<ObjectAttribute
						id="MB_PLAN_PERIOD"/>
				</attributes>
				 <!-- extension point for additional fields in header -->
                <core:ExtensionPoint name="extS5Header"></core:ExtensionPoint>
			</ObjectHeader>
			<IconTabBar
				expandable="true"
				id="TabContainer">
				<items>
					<IconTabFilter
						icon="sap-icon://hint"
						iconColor="Default"
						key="Information">
						<content>
							<VBox>
								<f:Form id="informationForm">
									<f:layout>
										<f:ResponsiveLayout id="informationLayout" />
									</f:layout>
									<f:formContainers>
										<f:FormContainer
											id="MB_LABEL_INFORMATION"
											title="{i18n>MB_INFORMATION}">
											<f:layoutData>
												<l:ResponsiveFlowLayoutData
													linebreak="true"
													margin="false"></l:ResponsiveFlowLayoutData>
											</f:layoutData>
											<f:formElements>
												<f:FormElement
													id="MB_FORM_NO_DATA"
													visible="true">
													<f:layoutData>
														<l:ResponsiveFlowLayoutData
															linebreak="true"
															margin="false"></l:ResponsiveFlowLayoutData>
													</f:layoutData>
													<f:label>
														<Label
															visible="true"
															id="MB_LABEL_NO_DATA"
															text="{i18n>MB_NO_DATA}">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="3"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Label>
													</f:label>
													<f:fields>
														<Text
															visible="true"
															id="MB_LABEL_NO_DATA_DUMMY">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="5"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Text>
													</f:fields>
