														</Label>
													</f:label>
													<f:fields>
														<Text
															visible="true"
															id="MB_BONUS_POST_TAX_PERCENT">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Text>
													</f:fields>
												</f:FormElement>
												<f:FormElement
													id="MB_FORM_BONUS_POST_TAX_DUMMY">
													<f:layoutData>
														<l:ResponsiveFlowLayoutData
															linebreak="true"
															margin="false"></l:ResponsiveFlowLayoutData>
													</f:layoutData>
													<f:label>
														<Label
															visible="true"
															id="MB_BONUS_POST_TAX_DUMMY">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Label>
													</f:label>
													<f:fields>
														<Text
															visible="true"
															id="MB_BONUS_POST_TAX_TEXT"
															text="{i18n>MB_START_POST_TAX_CONTRIBUTIONS}">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Text>
													</f:fields>
												</f:FormElement>
											</f:formElements>
										</f:FormContainer>
									</f:formContainers>
								</f:Form>
								<List
									visible="true"
									id="MB_LABEL_BENEFICIARIES"
									headerDesign="Plain"
									showSeparators="None"
									noDataText="{i18n>MB_NO_DATA}"
									headerText="{i18n>MB_BENEFICIARIES}"
									showNoData="true">
								</List>
								<List
									id="MB_LABEL_INVESTMENTS"
									headerText="{i18n>MB_INVESTMENTS}"
									noDataText="{i18n>MB_NO_DATA}">
									<columns>
										<Column
											hAlign="Left"
											minScreenWidth="Tablet">
											<!-- <header> <Label text="{i18n>TYPE}"></Label> </header> -->
										</Column>
										<Column
											hAlign="Center"
											minScreenWidth="Tablet"
											demandPopin="true">
											<!-- <header> <Label text="{i18n>DESCRIPTION}" /> </header> -->
										</Column>
										<Column
											hAlign="Right"
											minScreenWidth="Tablet"
											demandPopin="true">
											<!-- <header> <Text text="{i18n>TRC_PERCENTAGE}" /> </header> -->
										</Column>
									</columns>
								</List>
							</VBox>
						</content>
					</IconTabFilter>
					<IconTabFilter
						icon="sap-icon://attachment"
						iconColor="Default"
						key="Attachments"
						id="Attachment">
						<content>
							<List
								id="MB_LABEL_DOCUMENT"
								showSeparators="Inner"
								noDataText="{i18n>MB_NO_DATA}"
								items="{attachmentsModel>/0}">
								<StandardListItem
									type="Active"
									icon="sap-icon://chain-link"
									title="{attachmentsModel>UrlPlanText}"
									press="onSenderPress"></StandardListItem>
							</List>
						</content>
							</IconTabFilter>
						  <!-- extension added to add icon tab filter --> 
                <core:ExtensionPoint name="extS5IconTabFilter"></core:ExtensionPoint>
				</items>
			</IconTabBar>
		</content>
	</Page>
</core:View>
