<core:View
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:f="sap.ui.layout.form"
	xmlns:l="sap.ui.layout"
	xmlns:me="sap.me"
	controllerName="hcm.emp.mybenefits.view.S6">
	<Page
		title="{i18n>MB_APP_DETAIL_TITLE}"
		id="MB_PAGE_FSA">
		<content>
			<ObjectHeader
				id="MB_MISC_HEADER">
				<attributes>
					<ObjectAttribute
						id="MB_PLAN_SUB_TYPE_TEXT"
						text="{PlanSubTypeText}" />
					<ObjectAttribute
						id="MB_PLAN_OPTION_TEXT"
						text="{PlanOptionText}" />
					<ObjectAttribute
						id="MB_PLAN_PERIOD"
						text="{parts:[{path:'i18n>MB_PARTICIPATION_PERIOD'}, {path:'ParticipationBegin'}, {path:'ParticipationEnd'}], formatter:'utilFormatter.mbPeriod'}" />
				</attributes>
				<firstStatus>
					<ObjectStatus
						id="MB_PLAN_FSA_STATUS"
						></ObjectStatus>
				</firstStatus>
				 <!-- extension point for additional fields in header -->
                <core:ExtensionPoint name="extS6Header"></core:ExtensionPoint>
			</ObjectHeader>
			<IconTabBar
				expandable="true"
				id="TabContainer">
				<items>
					<IconTabFilter
						icon="sap-icon://hint"
						iconColor="Default"
						key="Information">
						<content>
							<VBox>
								<f:Form id="informationForm">
									<f:layout>
										<f:ResponsiveLayout id="informationLayout" />
									</f:layout>
									<f:formContainers>
										<f:FormContainer
											id="MB_LABEL_INFORMATION"
											title="{i18n>MB_INFORMATION}">
											<f:layoutData>
												<l:ResponsiveFlowLayoutData
													linebreak="true"
													margin="false"></l:ResponsiveFlowLayoutData>
											</f:layoutData>
											<f:formElements>
												<f:FormElement
													id="MB_FORM_NO_DATA"
													visible="true">
													<f:layoutData>
														<l:ResponsiveFlowLayoutData
															linebreak="true"
															margin="false"></l:ResponsiveFlowLayoutData>
													</f:layoutData>
													<f:label>
														<Label
															visible="true"
															id="MB_LABEL_NO_DATA"
															text="{i18n>MB_NO_DATA}">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="3"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Label>
													</f:label>
													<f:fields>
														<Text
															visible="true"
															id="MB_LABEL_NO_DATA_DUMMY">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="5"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Text>
													</f:fields>
												</f:FormElement>
												<f:FormElement
													id="MB_FORM_PLAN_NUMBER">
													<f:layoutData>
														<l:ResponsiveFlowLayoutData
															linebreak="true"
															margin="false"></l:ResponsiveFlowLayoutData>
													</f:layoutData>
													<f:label>
														<Label
															visible="true"
															id="MB_LABEL_PLAN_NUMBER"
															text="{i18n>MB_PLAN_NUMBER}">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Label>
													</f:label>
													<f:fields>
														<Text
															visible="true"
															id="MB_LABEL_PLAN_NUMBER_VALUE">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Text>
													</f:fields>
												</f:FormElement>
												<f:FormElement
													id="MB_FORM_PAYROLL_FREQUENCY">
													<f:layoutData>
														<l:ResponsiveFlowLayoutData
															linebreak="true"
															margin="false"></l:ResponsiveFlowLayoutData>
													</f:layoutData>
													<f:label>
														<Label
															visible="true"
															id="MB_LABEL_PAYROLL_FREQUENCY"
															text="{i18n>MB_PAYROLL_FREQUENCY}">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Label>
													</f:label>
													<f:fields>
														<Text
															visible="true"
															id="MB_LABEL_PAYROLL_FREQUENCY_VALUE">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Text>
													</f:fields>
												</f:FormElement>
												<f:FormElement
													id="MB_FORM_PLAN_OPTION">
													<f:layoutData>
														<l:ResponsiveFlowLayoutData
															linebreak="true"
															margin="false"></l:ResponsiveFlowLayoutData>
													</f:layoutData>
													<f:label>
														<Label
															visible="true"
															id="MB_LABEL_PLAN_OPTION"
															text="{i18n>MB_PLAN_OPTION}">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Label>
													</f:label>
													<f:fields>
														<Text
															visible="true"
															id="MB_LABEL_PLAN_OPTION_VALUE">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Text>
													</f:fields>
												</f:FormElement>
												<f:FormElement
													id="MB_FORM_PLAN_YEAR">
													<f:layoutData>
														<l:ResponsiveFlowLayoutData
															linebreak="true"
															margin="false"></l:ResponsiveFlowLayoutData>
													</f:layoutData>
													<f:label>
														<Label
															visible="true"
															id="MB_LABEL_PLAN_YEAR"
															text="{i18n>MB_PLAN_YEAR}">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Label>
													</f:label>
													<f:fields>
														<Text
															visible="true"
															id="MB_LABEL_PLAN_YEAR_VALUE">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Text>
													</f:fields>
												</f:FormElement>
												<f:FormElement
													id="MB_FORM_CONTRIBUTION">
													<f:layoutData>
														<l:ResponsiveFlowLayoutData
															linebreak="true"
															margin="false"></l:ResponsiveFlowLayoutData>
													</f:layoutData>
													<f:label>
														<Label
															visible="true"
															id="MB_LABEL_CONTRIBUTION"
															text="{i18n>MB_CONTRIBUTION_PER_PAY_PERIOD}">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Label>
													</f:label>
													<f:fields>
														<Text
															visible="true"
															id="MB_LABEL_CONTRIBUTION_VALUE">
															<layoutData>
																<l:ResponsiveFlowLayoutData
																	weight="4"></l:ResponsiveFlowLayoutData>
															</layoutData>
														</Text>
													</f:fields>
												</f:FormElement>
											</f:formElements>
										</f:FormContainer>
									</f:formContainers>
								</f:Form>
							</VBox>
						</content>
					</IconTabFilter>
					<IconTabFilter
						icon="sap-icon://attachment"
						iconColor="Default"
						key="Attachments"
						id="Attachment">
						<content>
							<List
								id="MB_LABEL_DOCUMENT"
								showSeparators="Inner"
								noDataText="{i18n>MB_NO_DATA}"
								items="{attachmentsModel>/0}">
								<StandardListItem
									type="Active"
									icon="sap-icon://chain-link"
									title="{attachmentsModel>UrlPlanText}"
									press="onSenderPress"></StandardListItem>
							</List>
						</content>
						
					</IconTabFilter>
					   <!-- extension added to add icon tab filter --> 
                <core:ExtensionPoint name="extS6IconTabFilter"></core:ExtensionPoint>
				</items>
			</IconTabBar>
		</content>
	</Page>
</core:View>
